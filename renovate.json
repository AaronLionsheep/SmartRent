{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "helpers:pinGitHubActionDigests"
  ],
  "packageRules": [
    {
      "matchManagers": ["pip_requirements"],
      "commitMessage": "{customType}({depName}): update {depName} to v{newVersion}",
      "customType": "deps"
    }
  ]
}
